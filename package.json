{
  "name": "anylist",
  "description": "ðŸ“‹ a wrapper for AnyList's API (unoffical, reverse engineered)",
  "keywords": [
    "ðŸ“‹",
    "anylist",
    "grocery",
    "list",
    "api",
    "wrapper",
    "client",
    "recipe"
  ],
  "version": "0.8.5",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "license": "MIT",
  "author": "Max Isom <hi@maxisom.me> (https://maxisom.me/)",
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "test": "npm run lint && npm run typecheck",
    "lint": "xo",
    "lint-fix": "xo --fix",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "document": "documentation build dist/index.js -f html -o docs"
  },
  "dependencies": {
    "form-data": "^3.0.0",
    "got": "^11.8.6",
    "protobufjs": "5.0.3",
    "reconnecting-websocket": "^4.2.0",
    "tough-cookie": "^3.0.1",
    "uuid": "^3.4.0",
    "ws": "^7.2.1"
  },
  "devDependencies": {
    "@types/form-data": "^2.2.1",
    "@types/node": "^24.0.12",
    "@types/protobufjs": "^6.0.0",
    "@types/tough-cookie": "^4.0.5",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "documentation": "^12.1.4",
    "husky": "^4.3.8",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "xo": "^0.56.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test && yarn run document"
    }
  },
  "xo": {
    "ignores": [
      "docs",
      "dist",
      "src"
    ],
    "rules": {
      "unicorn/prefer-module": "off",
      "unicorn/prefer-string-replace-all": "off",
      "unicorn/prefer-node-protocol": "off",
      "import/extensions": "off",
      "unicorn/prefer-event-target": "off"
    }
  },
  "files": [
    "dist",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codetheweb/anylist.git"
  },
  "bugs": {
    "url": "https://github.com/codetheweb/anylist/issues"
  },
  "homepage": "https://github.com/codetheweb/anylist",
  "engines": {
    "node": ">=16.0.0"
  }
}
